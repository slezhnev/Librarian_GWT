<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
	ui:generateLocales="default">
	<ui:style>
		/* Add CSS here. See the GWT docs on UI Binder for more details */
	</ui:style>
	<g:VerticalPanel width="100%" height="100%">
		<g:Cell horizontalAlignment="ALIGN_CENTER">
			<g:Button ui:field="refreshBtn" enabled="false">
				<ui:msg key="refresh">Refresh</ui:msg>
			</g:Button>
		</g:Cell>
		<g:Cell horizontalAlignment="ALIGN_CENTER">
			<g:Label text="statusLabelLibrary" ui:field="statusLabelLibrary" />
		</g:Cell>
		<g:Cell horizontalAlignment="ALIGN_CENTER">
			<g:Label text="statusLabel1" ui:field="statusLabelArcs" />
		</g:Cell>
		<g:Cell horizontalAlignment="ALIGN_CENTER">
			<g:Label text="statusLabel2" ui:field="statusLabelFiles" />
		</g:Cell>
		<g:Cell horizontalAlignment="ALIGN_CENTER">
			<g:Button ui:field="forceCheckBtn" enabled="false">
				<ui:msg key="forcecheck">Force check</ui:msg>
			</g:Button>
		</g:Cell>
	</g:VerticalPanel>
</ui:UiBinder> 