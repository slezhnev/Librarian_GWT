<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		/* Add CSS here. See the GWT docs on UI Binder for more details */
	</ui:style>
	<g:VerticalPanel width="100%" height="100%">
		<g:Cell horizontalAlignment="ALIGN_CENTER">
			<g:Button ui:field="refreshBtn" enabled="false">Refresh</g:Button>
		</g:Cell>
		<g:Label text="statusLabelLibrary" horizontalAlignment="ALIGN_CENTER" ui:field="statusLabelLibrary"/>
		<g:Cell horizontalAlignment="ALIGN_CENTER">
			<g:Label text="statusLabel1" ui:field="statusLabelArcs"/>
		</g:Cell>
		<g:Cell horizontalAlignment="ALIGN_CENTER">
			<g:Label text="statusLabel2" ui:field="statusLabelFiles"/>
		</g:Cell>
		<g:Cell horizontalAlignment="ALIGN_CENTER">
			<g:Button ui:field="forceCheckBtn" enabled="false">Force check</g:Button>
		</g:Cell>
	</g:VerticalPanel>
</ui:UiBinder> 